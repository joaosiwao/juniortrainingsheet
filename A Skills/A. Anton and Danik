#include <iostream>

using namespace std;

int main() {
    int n, a = 0, d = 0;
    cin >> n;
    string s;
    cin >> s;
    for(int i = 0; i < s.size();i++){
        if(s[i] == 'A'){
            a = a + 1;
        }if(s[i] == 'D'){
            d = d + 1;
        }
    }
    if(a > d){
        cout << "Anton";
    }
    if(d > a){
        cout << "Danik";
    }
    if(a == d){
        cout << "Friendship";
    }
}
