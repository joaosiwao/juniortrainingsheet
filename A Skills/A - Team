#include <iostream>

using namespace std;

int main() {
    int n, p, v, t;
    int o = 0;
    cin >> n;
    for (int i = 0; i < n; ++i) {
        cin >> p;
        cin >> v;
        cin >> t;
        if((p + v + t) == 2 or (p + v + t) == 3) {
            o = o + 1;
        }
    }
    cout << o;
}
